; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "ConfigFile",
"class": "Settings",
"language": "GDScript",
"path": "res://assets/menus/settings.gd"
} ]
_global_script_class_icons={
"Settings": ""
}

[application]

config/name="Thawing"
run/main_scene="res://assets/menus/main_menu.tscn"
config/icon="res://assets/packaging/icon.png"

[autoload]

InputController="*res://assets/ui/input_controller.gd"
DialogueTree="*res://assets/world/dialogue_tree.gd"
PersonnelData="*res://assets/characters/personnel_data.gd"

[editor]

search_in_file_extensions=PoolStringArray( "gd", "gdshader", "shader", "toml" )

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-accessibility/plugin.cfg" )

[importer_defaults]

texture={
"flags/filter": false
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.211765, 0.180392, 0.12549, 1 )
